{
  "name": "ENVY",
  "description": "Collects and send the visitor's browser environment values.",
  "keywords": [
    "Ruby",
    "Sinatra"
  ],
  "website": "https://github.com/kufu/envy",
  "repository": "https://github.com/kufu/envy",
  "env": {
    "CUSTOM_RUBY_VERSION": {
      "description": "Ruby version",
      "value": "2.4.1"
    },
    "SLACK_TOKEN": {
      "description": "Slack legacy token: https://api.slack.com/custom-integrations/legacy-tokens",
      "value": ""
    },
    "SLACK_CHANNEL": {
      "description": "Slack channel",
      "value": ""
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "image": "heroku/ruby"
}
